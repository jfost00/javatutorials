package data_types;

public class TypesandOperators {
	

	
	public static void main (String args[]) {
		long ci;
		long im; 
		
		im = 5280 * 12;
		ci = im * im * im;
		
		System.out.println("There are " + ci + " cubic inches in cubic mile.");
		
		double x, y, z;
		x = 3; 
		y = 4;
		z = Math.sqrt(x*x + y*y);
		System.out.println("Hypotenuse is " + z);
	}
}


